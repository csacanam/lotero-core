    "@x402/express": "^2.3.0",
    "@x402/fetch": "^2.3.0",
    "dotenv": "^16.4.5",
    "ethers": "^5.7.2",
    "express": "^4.21.0",
    "express-rate-limit": "^7.4.0",
    "viem": "^2.46.2"
  },
  "devDependencies": {
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3"
  },
  "resolutions": {
    "usehooks-ts@^2.7.2": "patch:usehooks-ts@npm:^2.7.2#./.yarn/patches/usehooks-ts-npm-2.7.2-fceffe0e43.patch"
  }
