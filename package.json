{
  "name": "se-2",
  "version": "0.0.1",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/contracts",
      "packages/frontend",
      "packages/agent"
    ]
  },
  "scripts": {
    "account": "yarn workspace @se-2/contracts account",
    "chain": "yarn workspace @se-2/contracts chain",
    "fork": "yarn workspace @se-2/contracts fork",
    "deploy": "yarn workspace @se-2/contracts deploy",
    "verify": "yarn workspace @se-2/contracts verify",
    "compile": "yarn workspace @se-2/contracts compile",
    "generate": "yarn workspace @se-2/contracts generate",
    "contracts:lint": "yarn workspace @se-2/contracts lint",
    "contracts:lint-staged": "yarn workspace @se-2/contracts lint-staged",
    "contracts:test": "yarn workspace @se-2/contracts test",
    "test": "yarn contracts:test",
    "start": "yarn workspace @se-2/frontend dev",
    "frontend:lint": "yarn workspace @se-2/frontend lint",
    "frontend:format": "yarn workspace @se-2/frontend format",
    "frontend:check-types": "yarn workspace @se-2/frontend check-types",
